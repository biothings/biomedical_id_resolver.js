{
  "name": "biomedical_id_resolver",
  "version": "2.0.0",
  "description": "js library for converting biomedical entity ids in batch",
  "main": "built/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "prettier --write \"src/**/*.ts\" \"built/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinxin90/biomedical_id_resolver.js.git"
  },
  "keywords": [
    "id",
    "conversion",
    "biomedical"
  ],
  "author": "jiwen xin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevinxin90/biomedical_id_resolver.js/issues"
  },
  "homepage": "https://github.com/kevinxin90/biomedical_id_resolver.js#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "istanbul": "^0.4.5",
    "jest": "^26.6.3",
    "mocha": "^6.2.3",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "axios": "^0.19.2",
    "camelcase": "^6.2.0",
    "debug": "^4.3.1",
    "husky": "^4.3.8",
    "lodash": "^4.17.20"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}